set_property PACKAGE_PIN R2 [get_ports clk]
set_property IOSTANDARD LVCMOS33 [get_ports clk]
create_clock -period 10.000 -name clk_100mhz -waveform {0.000 5.000} [get_ports clk]

set_property PACKAGE_PIN C18 [get_ports rst]
set_property IOSTANDARD LVCMOS33 [get_ports rst]
set_property PULLUP true [get_ports rst] 

set_property PACKAGE_PIN G15 [get_ports start_btn]
set_property IOSTANDARD LVCMOS33 [get_ports start_btn]
set_property PULLUP true [get_ports start_btn]

set_property PACKAGE_PIN R12 [get_ports tx]
set_property IOSTANDARD LVCMOS33 [get_ports tx]
